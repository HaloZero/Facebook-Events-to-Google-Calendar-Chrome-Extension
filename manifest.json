{
  "name": "Facebook Event to Google Calendar",
  "version": "1.0",
  "description": "Add individual facebook events to google calendar",
  "page_action": {
    "default_icon": "calendar_icon_16.png"
  },
  "icons": {
    "16" : "calendar_icon_16.png",
    "48" : "calendar_icon_48.png",
    "128" : "calendar_icon_128.png"
  },
  "permissions": [
    "http://*.facebook.com/*",
    "https://*.facebook.com/*",
    "http://*.google.com/*"
  ],
  "content_scripts": [
    {
      "matches" : ["https://graph.facebook.com/*"],
      "js" : ["jquery.js", "xdate.js", "facebook_event.js"]
    },
    {
      "matches" : ["https://www.facebook.com/connect/login_success.html*"],
      "js" : ["jquery.js", "xdate.js", "facebook_event.js"]
    },
    {
      "matches" : ["https://www.facebook.com/*"],
      "js" : ["jquery.js", "xdate.js", "facebook_event.js"]
    }
  ]
}
